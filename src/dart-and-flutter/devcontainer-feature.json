{
    "id": "dart-and-flutter",
    "version": "0.0.1",
    "name": "Dart and Flutter",
    "description": "Devcontainer feature for dart (and flutter)",
    "keywords": [
        "Dart",
        "Flutter"
    ],
    "installsAfter":[
        "ghcr.io/devcontainers/features/common-utils"
    ],
    "options": {
        "dart-version":{
            "type": "string",
            "description": "dart version to be installed.",
            "default": "latest"
        },
        "flutter":{
            "type": "boolean",
            "default": true
        }
    },
    "privileged": true
}